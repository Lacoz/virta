{
  "name": "@virta/examples",
  "version": "0.0.1",
  "description": "Example pipelines and demos for Virta DAG engine",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tsc --noEmit -p tsconfig.json",
    "example:basic": "tsx src/basic.ts",
    "example:parallel": "tsx src/parallel.ts",
    "example:error-handling": "tsx src/error-handling.ts",
    "example:hooks": "tsx src/hooks.ts",
    "example:jsonata": "tsx src/jsonata.ts",
    "example:jsonata-external": "tsx src/jsonata-external.ts",
    "example:registry": "tsx src/registry.ts",
    "example:asl": "tsx src/asl.ts",
    "example:arazzo": "tsx src/arazzo.ts",
    "example:arazzo-asl": "tsx src/arazzo-asl-conversion.ts",
    "example:arazzo-bpmn": "tsx src/arazzo-bpmn-conversion.ts",
    "example:bpmn": "tsx src/bpmn.ts",
    "example:planner": "tsx src/planner.ts",
    "example:arazzo-ts": "tsx src/arazzo-ts-conversion.ts",
    "example:pipeline-builder": "tsx src/pipeline-builder.ts",
    "example:runner-docker": "tsx src/runner-docker.ts",
    "example:runner-fallback": "tsx src/runner-fallback.ts",
    "example:runner-migration": "tsx src/runner-migration-docker.ts",
    "example:runner-split": "tsx src/runner-split-docker.ts",
    "example:runner-chain": "tsx src/runner-fallback-chain-docker.ts"
  },
  "dependencies": {
    "@virta/core": "workspace:*",
    "@virta/jsonata": "workspace:*",
    "@virta/registry": "workspace:*",
    "@virta/asl": "workspace:*",
    "@virta/arazzo": "workspace:*",
    "@virta/bpmn": "workspace:*",
    "@virta/planner": "workspace:*",
    "@virta/ts-codegen": "workspace:*",
    "@virta/runner": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}

